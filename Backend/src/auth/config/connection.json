          "ledgerQuery": true,
          "eventSource": true
        }
      }
    },
    "complianceentities": {
      "orderers": ["orderer0.pesachain.com"],
      "peers": {
        "peer0.pesachain.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
